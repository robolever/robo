<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Crypto Investment Dashboard</title>
  <link rel="shortcut icon" type="image/png" href="../assets/images/logos/favicon.png" />
  <link rel="stylesheet" href="../assets/css/styles.min.css" />
</head>

<body>
  <!--  Body Wrapper -->
  <div class="page-wrapper" id="main-wrapper" data-layout="vertical" data-navbarbg="skin6" data-sidebartype="full"
    data-sidebar-position="fixed" data-header-position="fixed">
    <!-- Sidebar Start -->
    <aside class="left-sidebar">
      <!-- Sidebar scroll-->
      <div>
        <div class="brand-logo d-flex align-items-center justify-content-between">
          <a href="/" class="text-nowrap logo-img">
            <img src="../assets/images/logos/logo.png" width="180" alt="" />
          </a>
          <div class="close-btn d-xl-none d-block sidebartoggler cursor-pointer" id="sidebarCollapse">
            <i class="ti ti-x fs-8"></i>
          </div>
        </div>
        <!-- Sidebar navigation-->
        <nav class="sidebar-nav scroll-sidebar" data-simplebar="">
          <ul id="sidebarnav">
            <li class="nav-small-cap">
              <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
              <span class="hide-menu">Home</span>
            </li>
            <li class="sidebar-item">
              <a class="sidebar-link" href="/" aria-expanded="false">
                <span>
                  <i class="ti ti-layout-dashboard"></i>
                </span>
                <span class="hide-menu">Dashboard</span>
              </a>
            </li>
            <li class="nav-small-cap">
              <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
              <span class="hide-menu">Investing</span>
            </li>
            <li class="sidebar-item">
              <a class="sidebar-link" href="/invest" aria-expanded="false">
                <span>
                  <i class="ti ti-shopping-cart"></i>
                </span>
                <span class="hide-menu">Make an Investment</span>
              </a>
              </li>
              <li class="sidebar-item">
                <a class="sidebar-link" href="/deposit-funds" aria-expanded="false">
                  <span>
                    <i class="ti ti-wallet"></i>
                  </span>
                  <span class="hide-menu">Make a Deposit</span>
                </a>
                </li>
            <li class="nav-small-cap">
              <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
              <span class="hide-menu">Membership</span>
            </li>
            <li class="sidebar-item">
              <a class="sidebar-link" href="/buy-premium" aria-expanded="false">
                <span>
                  <i class="ti ti-crown"></i>
                </span>
                <span class="hide-menu">Premium Membership</span>
              </a>
            </li>
            <li class="nav-small-cap">
              <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
              <span class="hide-menu">Withdrawals</span>
            </li>
            <li class="sidebar-item">
              <a class="sidebar-link" href="/withdrawal" aria-expanded="false">
                <span>
                  <i class="ti ti-cash"></i>
                </span>
                <span class="hide-menu">Withdraw Funds</span>
              </a>
            </li>
            <li class="sidebar-item">
              <a class="sidebar-link" href="/view-withdrawal" aria-expanded="false">
                <span>
                  <i class="ti ti-calendar"></i>
                </span>
                <span class="hide-menu">Withdrawal History</span>
              </a>
            </li>
            <li class="nav-small-cap">
              <i class="ti ti-dots nav-small-cap-icon fs-4"></i>
              <span class="hide-menu">Support</span>
            </li>
            <li class="sidebar-item">
              <a class="sidebar-link" href="/help-desk" aria-expanded="false">
                <span>
                  <i class="ti ti-help"></i>
                </span>
                <span class="hide-menu">Help Desk</span>
              </a>
            </li>
          </ul>
          
        </nav>
        <!-- End Sidebar navigation -->
      </div>
      <!-- End Sidebar scroll-->
    </aside>
    <!--  Sidebar End -->
    <!--  Main wrapper -->
    <div class="body-wrapper">
      <!--  Header Start -->
      <header class="app-header">
        <nav class="navbar navbar-expand-lg navbar-light">
          <ul class="navbar-nav">
            <li class="nav-item d-block d-xl-none">
              <a class="nav-link sidebartoggler nav-icon-hover" id="headerCollapse" href="javascript:void(0)">
                <i class="ti ti-menu-2"></i>
              </a>
            </li>
          </ul>
          <div class="navbar-collapse justify-content-end px-0" id="navbarNav">
            <ul class="navbar-nav flex-row ms-auto align-items-center justify-content-end">
              <li class="nav-item dropdown">
                <a class="nav-link nav-icon-hover" href="javascript:void(0)" id="drop2" data-bs-toggle="dropdown"
                  aria-expanded="false">
                  <img src="../assets/images/profile/user.png" alt="" width="35" height="35" class="rounded-circle">
                </a>
                <div class="dropdown-menu dropdown-menu-end dropdown-menu-animate-up" aria-labelledby="drop2">
                  <div class="message-body">
                    <a href="/profile" class="d-flex align-items-center gap-2 dropdown-item">
                      <i class="ti ti-user fs-6"></i>
                      <p class="mb-0 fs-3">My Profile</p>
                    </a>
                    
                    <a href="/users/logout" class="btn btn-outline-primary mx-3 mt-2 d-block">Logout</a>
                  </div>
                </div>
              </li>
            </ul>
          </div>
        </nav>
      </header>
      <!--  Header End -->
      <div class="container-fluid">
        <div id="dashboard" class="row">
          <div class="row">
            <div class="col-md-12 grid-margin">
              <div class="row">
                <div class="col-12 col-xl-8 mb-4 mb-xl-0">
                  <h2 class="font-weight-bold">Welcome Back, <%= " " 
                  + user.first_name %></h2>
                  <h6 class="font-weight-normal mb-0">General overview of Earnings, Transaction and Investment </h6>
                </div>
             
              </div>
            </div>
          </div>
          <div class="col-lg-12">
            <div class="row mt-4">
              <div class="col-lg-3">
                <!-- Total Investment -->
                <div class="card overflow-hidden">
                  <div class="card-body p-4">
                    <h5 class="card-title mb-1 fw-semibold">Total Deposit</h5>
                    <p class="text-dark mb-9 fs-3 mb-0">Available balance and equity</p>
                    <div class="row align-items-center">
                      <div class="col-12">
                        <h4 class="fw-semibold mb-3">$ <%= " " + user.totaldeposit %></h4>
                      </div>
                    
                    </div>
                  </div>
                </div>
              </div>
              <div class="col-lg-3">
                <!-- Total Earnings -->
                <div class="card">
                  <div class="card-body">
                    <div class="row alig n-items-start">
                      <div class="col-12">
                        <h5 class="card-title mb-1 fw-semibold">Plan Type</h5>
                        <p class="text-dark mb-9 fs-3 mb-0">Current investing plan</p>
                        <h4 class="fw-semibold mb-3"><%= user.plantype %></h4>
                        <div class="d-flex align-items-center pb-1">
                        </div>
                      </div>
                    </div>
                  </div>
                 
                </div>
              </div>
             
            </div>
          </div>
        </div>
        <div class="row mt-5" >
          <div class="row justify-content-center">
            <div class="col-12 content-head">
              <div class="mbr-section-head mb-5">
                <h5 class="mbr-section-title mbr-fonts-style align-center mb-0 display-5">
                  <strong>Investment Pricing plan</strong>
                </h5>
    
              </div>
            </div>
          </div>
          <div class="col-md-4">
             
             
              <div class="mb-3 text-center card card-body"><h2 class="card-title">Hypergrowth</h2>
                  <h2 class="widget-numbers text-primary">$1,000+</h2>
                  <hr>
                  <p class="page-title-subheading text-dark"  style="font-size: 1.4rem;">Up to 400% monthly ROI</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Duration of 1 - 4 months</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Weekly Bonuses</p>
                  <hr>
                  <a href="/select-coin-19843667164" class="btn btn-primary text-white" style="font-size: 1.4rem;">Invest  Now</a>
              </div>
              
          </div>
          <div class="col-md-4">
             
             
              <div class="mb-3 text-center card card-body">
                  <h5 class="card-title">Balanced Growth </h5>
                  <h2 class="widget-numbers text-primary">$5,000+</h2>
                  <hr>
                  <p class="page-title-subheading text-dark"  style="font-size: 1.4rem;">Up to 750% quarterly ROI</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Duration of 3 - 6 months</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Monthly bonuses</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Spark dividends</p>
                  <hr>
                  <a class="btn btn-primary text-white" href="/select-coin-19843667164" style="font-size: 1.4rem;">Invest Now </a>
                  
              </div>
              
          </div>
          <div class="col-md-4">
             
             
              <div class="mb-3 text-center card card-body"><h5 class="card-title">Strategic Growth</h5>
                <h2 class="widget-numbers text-primary">$10,000+</h2>
                  <hr>
                  <p class="page-title-subheading text-dark"  style="font-size: 1.4rem;">Up to 1250% Yearly ROI</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Duration of 9 - 16 months</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Monthly bonuses</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Spark dividends</p>
                  <hr>
                  <p class="page-title-subheading text-dark" style="font-size: 1.4rem;">Annual dividends</p>
                  <hr>
                  <a class="btn btn-primary text-white" href="/select-coin-19843667164" style="font-size: 1.4rem;">Invest  Now</a>
                  
              </div>
              
          </div>
         

      </div>
      </div>
      
    </div>
  </div>
  <script src="../assets/libs/jquery/dist/jquery.min.js"></script>
  <script src="../assets/libs/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
  <script src="../assets/js/sidebarmenu.js"></script>
  <script src="../assets/js/app.min.js"></script>
  <script src="../assets/libs/apexcharts/dist/apexcharts.min.js"></script>
  <script src="../assets/libs/simplebar/dist/simplebar.js"></script>
  <script src="../assets/js/dashboard.js"></script>
  <% include ./partials/chat %>
</body>

</html>